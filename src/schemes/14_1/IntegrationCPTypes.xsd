									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="authorityPrescriptionInfo" type="authorityPrescriptionType">
							<xs:annotation>
								<xs:documentation>Предписание органа, уполномоченного на осуществление контроля</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="courtDecisionInfo" type="courtDecisionType">
							<xs:annotation>
								<xs:documentation>Решение судебного органа</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>

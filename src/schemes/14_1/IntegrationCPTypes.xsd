											<xs:annotation>
												<xs:documentation>Телефон</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="informationPersonsInfo" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о лицах, имеющих право без доверенности действовать от имени юридического лица РФ</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="informationPersonInfo" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Сведения о лице, имеющем право без доверенности действовать от имени юридического лица</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="nameInfo" type="cmn:personType">

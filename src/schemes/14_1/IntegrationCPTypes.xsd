																		<xs:documentation>ИНН</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="position" type="base:positionType">
																	<xs:annotation>
																		<xs:documentation>Должность</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="filialLegalEntityRFInfo">
								<xs:annotation>
									<xs:documentation>Обособленное подразделение юридического лица РФ</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="legalEntityInfo">

<?xml version="1.0" encoding="UTF-8"?>
<!-- EIS INTEGRATION_EP_TYPES Integration Scheme, version 14.1, create date 26.03.2024 -->
<xs:schema xmlns:base="http://zakupki.gov.ru/oos/base/1" xmlns="http://zakupki.gov.ru/oos/EPtypes/1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cmn="http://zakupki.gov.ru/oos/common/1" targetNamespace="http://zakupki.gov.ru/oos/EPtypes/1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://zakupki.gov.ru/oos/base/1" schemaLocation="BaseTypes.xsd"/>
	<xs:import namespace="http://zakupki.gov.ru/oos/common/1" schemaLocation="CommonTypes.xsd"/>
	<!--Основные документы-->
	<!--Общие документы-->
	<xs:complexType name="noticeApplicationCancelType">
		<xs:annotation>
			<xs:documentation>Тип: Уведомление об отзыве заявки участником</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа ЕИС. Игнорируется при приеме-передаче. Добавлено на развитие </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalId" type="base:externalIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Внешний идентификатор документа. При приеме контролируется уникальность номера в рамках организации, размещающей закупку</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commonInfo">
				<xs:annotation>
					<xs:documentation>Общая информация</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="purchaseNumber" type="base:purchaseNumberType">
							<xs:annotation>
								<xs:documentation>Реестровый номер закупки. При приёме контролируется наличие неотменённой (в случае документа notificationCancelFailure наоборот отмененной) закупки с указанным номером</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="docNumber" type="base:documentNumberType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер документа.
Элемент игнорируется при приёме. Заполняется при передаче номером документа, присвоенным в ЕИС</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="docPublishDTInEIS" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата размещения документа в ЕИС. Элемент игнорируется при приёме. При передаче заполняется датой размещения документа в ЕИС</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="href" type="base:hrefType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Гиперссылка на опубликованный документ</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="extPrintFormInfo" type="cmn:extPrintFormType">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="applicationInfo">
				<xs:annotation>
					<xs:documentation>Информация о заявке</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="appNumber" type="base:journalNumberType">
							<xs:annotation>
								<xs:documentation>Номер заявки</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="appDT" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>Дата и время подачи заявки</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="appRating" type="xs:short">
							<xs:annotation>
								<xs:documentation>Порядковый номер заявки</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="protocolInfo">
				<xs:annotation>
					<xs:documentation>Информация о связанном протоколе</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="protocolNumber" type="base:documentNumberType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Сформированный в ЕИС номер протокола. Игнорируется при приёме. При передаче заполняется номером протокола сформированным в ЕИС</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="protocolNumberExternal" type="base:documentNumberType">
							<xs:annotation>
								<xs:documentation>Сформированный во внешней системе номер протокола. При приёме контролируется наличие опубликованного протокола с указанным номером</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="versionNumber" type="base:versionNumberType">
							<xs:annotation>
								<xs:documentation>Номер версии протокола. При приёме контролируется наличие опубликованного протокола с указанной версией</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="hrefExternal" type="base:hrefType">
				<xs:annotation>
					<xs:documentation>Гиперссылка на размещённый документ во внешней системе</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="notificationCancelType">
		<xs:annotation>
			<xs:documentation>Тип: Извещение об отмене определения поставщика (подрядчика, исполнителя)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="purchaseDocumentType">
				<xs:sequence>
					<xs:element name="cancelReasonInfo" type="notificationChangeReasonType">
						<xs:annotation>
							<xs:documentation>Причина отмены</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="addInfo" type="base:text2000Type" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Дополнительная информация</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="serviceSigns" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Служебные признаки</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="isIncludeKOKS" type="xs:boolean" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Признак «Код объекта капитального строительства/ недвижимого имущества» Игнорируется при приеме, заполняется автоматически, если в связанной закупке в блоке "Этапы исполнения контракта." (stagesInfo) указан код по справочнику "Классификатор объектов капитального строительства" (КОКС) (nsiKOKS)</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="notificationCancelFailureType">
		<xs:annotation>
			<xs:documentation>Тип: Отмена извещения об отмене определения поставщика (подрядчика, исполнителя)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="purchaseDocumentType">
				<xs:sequence>
					<xs:element name="cancelFailureInfo">
						<xs:annotation>
							<xs:documentation>Информация об отмене извещения об отмене определения поставщика (подрядчика, исполнителя)</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="notificationCancelInfo" type="base:text2000Type" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Информация об отменяемом извещении об отмене определения поставщика </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="placingWay" type="base:placingWayRef" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Подспособ определения поставщика</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="purchaseObjectInfo" type="base:text2000Type" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Наименование объекта закупки</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="recoveryToStage" type="base:tenderStageType">
									<xs:annotation>
										<xs:documentation>Этап определения поставщика

Допустимые значения:

NP  Подготовка извещения;
AP  Подача заявок;
CW  Работа комиссии;
FO  Определение поставщика завершено;
CO  Определение поставщика отменено.		</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="notificationCancelFailureOrgInfo">
									<xs:annotation>
										<xs:documentation>Организация, осуществляющая размещение отмены извещения определения поставщика (подрядчика, исполнителя)</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="organizationType">
												<xs:sequence>
													<xs:element name="responsibleRole" type="base:responsibleRoleEPType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Роль организации, осуществляющей закупку
CU - Заказчик;
OCU - Заказчик в качестве организатора совместного аукциона;
RA - Уполномоченный орган;
ORA- Уполномоченный орган в качестве организатора совместного конкурса (аукциона) согласно ст. 25 №44ФЗ;
AI - Уполномоченное учреждение;
OAI- Уполномоченное учреждение в качестве организатора совместного конкурса (аукциона) согласно ст. 25 №44ФЗ;
OA - Организация, осуществляющая полномочия заказчика на основании соглашения в соответствии с частью 6 статьи 15 Федерального закона № 44-ФЗ;
OOA- Организация, осуществляющая полномочия заказчика на осуществление закупок на основании договора (соглашения) в качестве организатора совместного конкурса (аукциона) согласно ст. 25 №44ФЗ;
CS - Организация, осуществляющая закупки в соответствии с частью 5 статьи 15 Федерального закона № 44-ФЗ;
OCS -  Заказчик, осуществляющий закупки в соответствии с частью 5 статьи 15 Федерального закона № 44-ФЗ, в качестве организатора совместного конкурса (аукциона) согласно ст. 25 №44ФЗ;
CC - Заказчик по Федеральному закону 223-ФЗ, осуществляющий закупки в соответствии с Федеральным законом № 44-ФЗ, в случаях, предусмотренных Федеральным законом № 223-ФЗ;
OCC - Заказчик, осуществляющий закупки в соответствии с Федеральным законом № 44-ФЗ, в связи с неразмещением положения о закупке в соответствии с положениями Федерального закона № 223-ФЗ,
в качестве организатора совместного конкурса (аукциона) согласно ст. 25 №44ФЗ;
AU - Юридическое лицо, осуществляющее закупку в соответствии с частью 4 статьи 5 Федерального закона от 30.12.2008 № 307-ФЗ «Об аудиторской деятельности»;
OAU - Заказчик, осуществляющий закупку на проведение обязательного аудита (код AU), в качестве организатора совместного конкурса (аукциона) согласно ст. 25 №44ФЗ;
RO - Специализированная некоммерческая организация, которая осуществляет деятельность, направленную на обеспечение проведения капитального ремонта общего имущества в многоквартирных домах в соответствии с Постановлением № 615 (региональный оператор);
CN - Организация, осуществляющая закупки в соответствии с частью 4.1 или 4.3 статьи 15 Федерального закона N 44-ФЗ;
OCN - Заказчик, осуществляющий закупки в соответствии с частью 4.1 статьи 15 Федерального закона № 44-ФЗ в качестве организатора совместного конкурса (аукциона) согласно ст. 25 №44ФЗ;
CU5CH26 - Заказчик как орган исполнительной власти по ч.5 ст.26 Закона № 44-ФЗ.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="cancelReasonInfo" type="changeReasonType">
									<xs:annotation>
										<xs:documentation>Основание для отмены извещений об отмене определения поставщика (подрядчика, исполнителя)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="addInfo" type="base:text2000Type" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Дополнительная информация</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="serviceSigns" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Служебные признаки</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="isIncludeKOKS" type="xs:boolean" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Признак «Код объекта капитального строительства/ недвижимого имущества» Игнорируется при приеме, заполняется автоматически, если в связанной закупке в блоке "Этапы исполнения контракта." (stagesInfo) указан код по справочнику "Классификатор объектов капитального строительства" (КОКС) (nsiKOKS)</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="clarificationDocRequestType">
		<xs:annotation>
			<xs:documentation>Тип: Запрос о даче разъяснений положений извещения об осуществлении закупки</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="clarificationRequestType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="clarificationDocType">
		<xs:annotation>
			<xs:documentation>Тип:Разъяснения положений извещения об осуществлении закупки</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="clarificationType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="clarificationResultRequestType">
		<xs:annotation>
			<xs:documentation>Тип: Запрос о даче разъяснений результатов (не применяется для СОП EF2020, EOK2020, EZK2020, EZT)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="clarificationRequestType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="clarificationResultType">
		<xs:annotation>
			<xs:documentation>Тип: Разъяснение результатов (не применяется для СОП EF2020, EOK2020, EZK2020, EZT)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="clarificationType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="protocolCancelType">
		<xs:annotation>
			<xs:documentation>Тип: Информация об отмене протокола, принятого от ЭТП</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа ЕИС. Игнорируется при приеме-передаче. Добавлено на развитие </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalId" type="base:externalIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Внешний идентификатор документа. При приеме контролируется уникальность номера в рамках организации, размещающей закупку</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commonInfo">
				<xs:annotation>
					<xs:documentation>Общая информация</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="purchaseNumber" type="base:purchaseNumberType">
							<xs:annotation>
								<xs:documentation>Реестровый номер закупки. При приёме контролируется наличие неотменённой закупки с указанным номером</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="docNumber" type="base:documentNumberType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер документа. Элемент игнорируется при приёме. Заполняется при передаче номером документа, присвоенным в ЕИС</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="publishDTInEIS" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата и время размещения документа в ЕИС. Элемент игнорируется при приёме. При передаче заполняется датой размещения документа в ЕИС</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="href" type="base:hrefType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Гиперссылка на размещённый в ЕИС документ. Элемент игнорируется при приёме. При передаче заполняется ссылкой на карточку размещенного документа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="publishDTInETP" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>Дата и время размещения документа на ЭТП</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="signDate" type="xs:date">
							<xs:annotation>
								<xs:documentation>Дата подписания документа на ЭТП</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="hrefExternal" type="base:hrefType">
							<xs:annotation>
								<xs:documentation>Гиперссылка на размещённый документ во внешней системе</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="canceledProtocolType" type="base:protocolTypeEnum">
							<xs:annotation>
								<xs:documentation>Тип отменяемого протокола</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="canceledProtocolNumber" type="base:documentNumberType">
							<xs:annotation>
								<xs:documentation>Сформированный во внешней системе номер отменяемого протокола. При приёме контролируется наличие опубликованного протокола с указанным номером</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="addInfo" type="base:text2000Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="printFormInfo" type="cmn:printFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Печатная форма документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extPrintFormInfo" type="cmn:extPrintFormType">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="attachmentsInfo" type="cmn:attachmentListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о прикрепленных документах.
При приеме контролируется обязательность заполнения блока "Электронная подпись" (attachmentsInfo/attachmentInfo/cryptoSigns)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cancelReasonInfo" type="cancelWithoutCustomerDecisionType">
				<xs:annotation>

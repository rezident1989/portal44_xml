										</xs:annotation>
										<xs:complexType>
											<xs:choice>
												<xs:element name="KLADRInfo" type="kladrPlaceType" maxOccurs="unbounded">
													<xs:annotation>
														<xs:documentation>Место доставки товара, выполнения работы или оказания услуги по справочнику КЛАДР</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="OKTMOInfo" maxOccurs="unbounded">
													<xs:annotation>
														<xs:documentation>Место доставки товара, выполнения работы или оказания услуги по справочнику ОКТМО</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="OKTMO" type="base:OKTMORef">
																<xs:annotation>
																	<xs:documentation>Территориально-муниципальное образование организации по ОКТМО</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="deliveryPlace" type="base:text2000Type">
																<xs:annotation>
																	<xs:documentation>Место</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:choice>
										</xs:complexType>
									</xs:element>
									<xs:element name="deliveryTerm" type="base:text2000Type" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Сроки доставки товара, выполнения работы или оказания услуги либо график оказания услуг</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="customers" minOccurs="0">

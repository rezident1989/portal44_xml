	<xs:complexType name="kladrType">
		<xs:annotation>
			<xs:documentation>Тип: Адрес по КЛАДР</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="kladrCode" type="base:kladrCodeType">

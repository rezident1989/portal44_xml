			<xs:documentation>Тип: Отмена документа закупки без отмены на по решению заказчика</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="info" type="base:text2000Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Краткое описание</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addInfo" type="base:text2000Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reasonInfo" type="changeReasonWithoutCustomerDecisionType">

					<xs:documentation>Дата и время размещения документа на ЭТП</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="place" type="base:text2000Type">
				<xs:annotation>
					<xs:documentation>Место проведения процедуры</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="procedureDT" type="xs:dateTime">

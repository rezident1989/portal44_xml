																			<xs:documentation>Значение дозировки</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="dosageGRLSValue" type="base:drugNameType">
																		<xs:annotation>
																			<xs:documentation>Полная форма дозировки</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
														<xs:element name="packagingInfo" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Сведения об упаковке. Игнорируется при приеме, заполняется при передаче из справочника "Лекарственные препараты" (nsiFarmDrugDictionary)</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="primaryPackagingInfo" minOccurs="0">

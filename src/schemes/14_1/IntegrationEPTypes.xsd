																						<xs:documentation>Оценка заявки</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																				<xs:element name="appRating" type="xs:short">
																					<xs:annotation>
																						<xs:documentation>Порядковый номер заявки по результатам оценки заявки </xs:documentation>
																					</xs:annotation>
																				</xs:element>
																				<xs:element name="conditionsScoringInfo">
																					<xs:annotation>
																						<xs:documentation>Оценка предложения участника по критериям оценки</xs:documentation>
																					</xs:annotation>
																					<xs:complexType>
																						<xs:sequence>
																							<xs:element name="conditionScoringInfo" maxOccurs="unbounded">
																								<xs:annotation>
																									<xs:documentation>Оценка предложения участника по отдельному критерию оценки, В рамках блока должен присутствовать как минимум один блок costCriterionInfo </xs:documentation>
																								</xs:annotation>
																								<xs:complexType>
																									<xs:choice>
																										<xs:element name="costCriterionInfo" type="costCriterionProtocolOKType">
																											<xs:annotation>
																												<xs:documentation>Стоимостной критерий оценки</xs:documentation>
																											</xs:annotation>
																										</xs:element>
																										<xs:element name="qualitativeCriterionInfo" type="qualitativeCriterionProtocolOK32020Type">
																											<xs:annotation>
																												<xs:documentation>Нестоимостной критерий оценки</xs:documentation>
																											</xs:annotation>
																										</xs:element>
																									</xs:choice>
																								</xs:complexType>
																							</xs:element>
																						</xs:sequence>
																					</xs:complexType>
																				</xs:element>
																				<xs:element name="finalDT" type="xs:dateTime" minOccurs="0">
																					<xs:annotation>
																						<xs:documentation>Дата и время подачи окончательного предложения</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																				<xs:element name="rightConcludeContractPrice" type="xs:boolean">
																					<xs:annotation>
																						<xs:documentation>Цена за право заключения контракта. Обязательно и допустимо указание значения true при отрицательном значении ценового предложения участника</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																			</xs:sequence>
																		</xs:complexType>
																	</xs:element>
																	<xs:element name="appNotAdmittedInfo">

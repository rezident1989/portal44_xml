																						<xs:documentation>Дата и время подачи окончательного предложения</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																				<xs:element name="rightConcludeContractPrice" type="xs:boolean">
																					<xs:annotation>
																						<xs:documentation>Цена за право заключения контракта. Обязательно и допустимо указание значения true при отрицательном значении ценового предложения участника</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																			</xs:sequence>
																		</xs:complexType>
																	</xs:element>
																	<xs:element name="appAdmittedInfo">
																		<xs:annotation>
																			<xs:documentation>Информация о допущенной заявке (В случае если допущено более одной заявки)</xs:documentation>
																		</xs:annotation>
																		<xs:complexType>

					<xs:documentation>Краткое описание</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addInfo" type="base:text2000Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reasonInfo" type="changeReasonWithoutCustomerDecisionType">
				<xs:annotation>
					<xs:documentation>Основание</xs:documentation>
				</xs:annotation>

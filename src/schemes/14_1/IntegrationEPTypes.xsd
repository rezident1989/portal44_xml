				</xs:element>
				<xs:element name="min" type="base:decimalKTRUCharacteristicValue18p11Type" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Минимальное значение диапазона</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:annotation>
					<xs:documentation>В случае отсутствия должна быть задана нижняя граница диапазона (minMathNotation + min)</xs:documentation>
				</xs:annotation>
				<xs:element name="maxMathNotation" type="base:ktruMaxMathNotationType" minOccurs="0">

			<xs:enumeration value="notification"/>
			<xs:enumeration value="protocol"/>
			<xs:enumeration value="contractProject"/>
			<xs:enumeration value="contractProjectChange"/>
			<xs:enumeration value="tenderPlan2020"/>
			<xs:enumeration value="acceptableBO"/>
			<xs:enumeration value="notification2022"/>
			<xs:enumeration value="notificationAcceptableBO"/>
			<xs:enumeration value="acceptedBO"/>
			<xs:enumeration value="contractAcceptedBO"/>

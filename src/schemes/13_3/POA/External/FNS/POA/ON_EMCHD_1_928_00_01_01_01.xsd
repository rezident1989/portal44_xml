<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2006 sp2 U (http://www.altova.com) by VI (GNIVC FNS RF) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn://x-artefacts/EMCHD_1" targetNamespace="urn://x-artefacts/EMCHD_1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Доверенность">
		<xs:annotation>
			<xs:documentation>Сведения о доверенности</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ИнСвед" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Иные сведения</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="5000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Документ">
					<xs:annotation>
						<xs:documentation>Состав и структура документа</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice>
							<xs:element name="Довер">
								<xs:annotation>
									<xs:documentation>Доверенность</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СвДов" type="СвДовТип">
											<xs:annotation>
												<xs:documentation>Сведения о доверенности</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвДоверит" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сведения о доверителе (доверителях)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Доверит">
														<xs:annotation>
															<xs:documentation>Сведения о доверителе</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="РосОргДовер" type="РосОргДоверТип" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Сведения о юридическом лице</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ИнОргДовер" type="ИнОргДоверТип" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Сведения об иностранном юридическом лице</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ИПДовер" type="СведИПТип" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Сведения об индивидуальном предпринимателе</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ФЛДовер" type="ФЛДоверТип" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Сведения о физическом лице</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="ТипДоверит" use="required">
													<xs:annotation>
														<xs:documentation>Тип доверителя</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:length value="1"/>
															<xs:enumeration value="1"/>
															<xs:enumeration value="2"/>
															<xs:enumeration value="3"/>
															<xs:enumeration value="4"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="СвУпПред" type="СвУпПредТип" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сведения о представителе (представителях)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвПолн" type="СвПолнТип">
											<xs:annotation>
												<xs:documentation>Сведения о полномочиях представителя (представителей)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвНотУд" type="СвНотУдТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о нотариальном удостоверении</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Передов">
								<xs:annotation>
									<xs:documentation>Передоверие</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СвПервДовер" type="СвПервДоверТип">
											<xs:annotation>
												<xs:documentation>Сведения о первоначальной доверенности</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвПередов" type="СвПервДоверТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о доверенности, на основании которой осуществляется передоверие</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвПереДовер" type="СвДовТип">
											<xs:annotation>
												<xs:documentation>Сведения о доверенности</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвПередПолн" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сведения о лице (лицах), передавшем (передавших) полномочия</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ПередПолн">
														<xs:annotation>
															<xs:documentation>Сведения о лице, передавшем полномочия</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="РосОргПерПолн" type="РосОргДоверТип" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Сведения о юридическом лице</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ИППерПолн" type="СведИПТип" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Сведения об индивидуальном предпринимателе</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ФЛПерПолн" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Сведения о физическом лице</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="СведРукоп" type="СвФЛТип" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Сведения о рукоприкладчике</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="СведФЛ" type="СведФЛТип">
																				<xs:annotation>
																					<xs:documentation>Сведения о физическом лице</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																		<xs:attribute name="СтУчНД" use="optional">
																			<xs:annotation>
																				<xs:documentation>Статус участника нотариального действия</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:length value="3"/>
																					<xs:enumeration value="101"/>
																					<xs:enumeration value="102"/>
																					<xs:enumeration value="299"/>
																					<xs:enumeration value="301"/>
																					<xs:enumeration value="303"/>
																					<xs:enumeration value="399"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="ПрНалРук" use="optional">
																			<xs:annotation>
																				<xs:documentation>Признак наличия рукоприкладчика</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:length value="1"/>
																					<xs:enumeration value="0"/>
																					<xs:enumeration value="1"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="ИННФЛ" type="ИННФЛТип" use="required">
																			<xs:annotation>
																				<xs:documentation>ИНН физического лица</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute name="СНИЛС" type="СНИЛСТип" use="required">
																			<xs:annotation>
																				<xs:documentation>СНИЛС</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																	</xs:complexType>
																</xs:element>
																<xs:element name="ФилПерПолн" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Сведения о филиале (обособленном подразделении) юридического лица</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="СвФил" type="СвОргТип">
																				<xs:annotation>
																					<xs:documentation>Сведения о филиале (обособленном подразделении) юридического лица</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="СвРукФил" type="СвФЛТип">
																				<xs:annotation>
																					<xs:documentation>Сведения о руководителе филиала (обособленном подразделении) юридического лица</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="ИнПерПолн" type="ИнОргДоверТип" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Сведения о филиале (аккредитованном представительстве) иностранного юридического лица</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="ТипПерПолн" use="required">
													<xs:annotation>
														<xs:documentation>Тип лица, передавшего полномочия</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:length value="1"/>
															<xs:enumeration value="1"/>
															<xs:enumeration value="2"/>
															<xs:enumeration value="3"/>
															<xs:enumeration value="4"/>
															<xs:enumeration value="5"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="СвПолучПолн" type="СвУпПредТип" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сведения о лице (лицах), получившем (получивших) полномочия</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвПолн" type="СвПолнТип">
											<xs:annotation>
												<xs:documentation>Сведения о передаваемом (передаваемых) полномочии (полномочиях)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвНотУд" type="СвНотУдТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о нотариальном удостоверении</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="ПрНалПередов" use="required">
										<xs:annotation>
											<xs:documentation>Признак наличия доверенности, выданной в порядке передоверия</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="1"/>
												<xs:enumeration value="0"/>
												<xs:enumeration value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:choice>
						<xs:attribute name="КНД" use="optional">
							<xs:annotation>
								<xs:documentation>Код формы по КНД</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="7"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ВерсФорм" use="required">
				<xs:annotation>
					<xs:documentation>Версия формата</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="10"/>
						<xs:enumeration value="EMCHD_1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ПрЭлФорм" use="required">
				<xs:annotation>
					<xs:documentation>Флаг обязательности элементов </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="8"/>
						<xs:pattern value="[0-1]{8}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ИдФайл" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор файла</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ИдФайлНО" use="optional">
				<xs:annotation>
					<xs:documentation>Идентификатор файла для представления интересов в налоговом органе</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="OID" use="optional">
				<xs:annotation>
					<xs:documentation>Внешний идентификатор объекта ЕСИА</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="РосОргДоверТип">
		<xs:annotation>
			<xs:documentation>Сведения о юридическом лице</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="СвРосОрг" type="СвОргТип">
				<xs:annotation>
					<xs:documentation>Сведения о юридическом лице</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ЛицоБезДов" type="ЛицоБезДовТип" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Сведения о лице (лицах), действующем (действующих) от имени юридического лица без доверенности</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ЕИОУК" use="required">
			<xs:annotation>
				<xs:documentation>Единоличным исполнительным органом выступает управляющая компания</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="1"/>
					<xs:enumeration value="0"/>
					<xs:enumeration value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ЕИОФЛ" use="required">
			<xs:annotation>
				<xs:documentation>Единоличным исполнительным органом выступает физическое лицо</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="1"/>
					<xs:enumeration value="0"/>
					<xs:enumeration value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ЕИОИП" use="required">
			<xs:annotation>
				<xs:documentation>Единоличным исполнительным органом выступает индивидуальный предприниматель</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="1"/>
					<xs:enumeration value="0"/>
					<xs:enumeration value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ЛицоБезДовТип">
		<xs:annotation>
			<xs:documentation>Сведения о лице (лицах), в том числе законном представителе, действующем (действующих) без доверенности</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="СВЮЛ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о единоличном исполнительном органе – управляющей компании</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="СвЮЛЕИО" type="СвОргТип">
							<xs:annotation>
								<xs:documentation>Сведения о юридическом лице</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="СвФЛ" type="СвФЛТип" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Сведения о физическом лице (физических лицах), действующем (действующих) от имени юридического лица без доверенности</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="СвФЛ" type="СвФЛТип" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о единоличном исполнительном органе - физическом лице</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="СвИП" type="СведИПТип" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о единоличном исполнительном органе - индивидуальном предпринимателе</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ПолнЮЛ" use="required">
			<xs:annotation>
				<xs:documentation>Вид полномочий единоличного исполнительного органа</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="1"/>
					<xs:enumeration value="1"/>
					<xs:enumeration value="2"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="СвФЛТип">
		<xs:annotation>
			<xs:documentation>Сведения о физическом лице / руководителе юридического лица (иностранного юридического лица (или филиала (аккредитованного представительства) иностранного юридического лица))</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ДокПдтв" type="ДокПдтвТип" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документ, подтверждающий полномочия лица, действующего без доверенности / руководителя иностранного юридического лица / руководителя филиала (аккредитованного представительства) иностранного юридического лица / руководителя филиала (обособленного подразделения) юридического лица</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="СведФЛ" type="СведФЛТип">
				<xs:annotation>
					<xs:documentation>Сведения о физическом лице</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="СтУчНД" use="optional">
			<xs:annotation>
				<xs:documentation>Статус участника нотариального действия</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="3"/>
					<xs:enumeration value="101"/>
					<xs:enumeration value="102"/>
					<xs:enumeration value="299"/>
					<xs:enumeration value="301"/>
					<xs:enumeration value="303"/>
					<xs:enumeration value="399"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ИННФЛ" type="ИННФЛТип" use="optional">
			<xs:annotation>
				<xs:documentation>ИНН физического лица</xs:documentation>
			</xs:annotation>
		</xs:attribute>
